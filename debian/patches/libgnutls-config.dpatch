+AC_MSG_CHECKING([whether to enable ACL support in neon])
+if test "x$neon_no_acl" = "xyes"; then
+    AC_MSG_RESULT(no)
+else
+    AC_MSG_RESULT(yes)
+    NEON_EXTRAOBJS="$NEON_EXTRAOBJS ne_oldacl ne_acl3744"
+fi
+
+NEON_SSL()
+NEON_GSSAPI()
+NEON_LIBPROXY()
+
+AC_SUBST(NEON_CFLAGS)
+AC_SUBST(NEON_LIBS)
+AC_SUBST(NEON_LTLIBS)
+
+])
